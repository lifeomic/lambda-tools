{
  "name": "@lifeomic/lambda-tools",
  "version": "3.0.4",
  "description": "Common utilities for Lambda testing and development",
  "main": "src/index.js",
  "bin": {
    "lambda-tools-build": "./bin/build.js",
    "lambda-tools-host-addr": "./bin/get-host-addr.js"
  },
  "scripts": {
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "pretest": "eslint .",
    "test": "nyc ava -v"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lifeomic/lambda-tools.git"
  },
  "author": "LifeOmic <development@lifeomic.com>",
  "license": "MIT",
  "ava": {
    "failWithoutAssertions": false,
    "files": [
      "test/**/*.test.js"
    ]
  },
  "eslintConfig": {
    "extends": "plugin:@lifeomic/node/recommended"
  },
  "eslintIgnore": [
    "examples/",
    "test/fixtures/bundled_*",
    "test/fixtures/build"
  ],
  "nyc": {
    "check-coverage": true,
    "lines": 100,
    "statements": 100,
    "functions": 100,
    "branches": 100
  },
  "devDependencies": {
    "@lifeomic/eslint-plugin-node": "^1.1.1",
    "@types/koa": "^2.0.45",
    "@types/koa-router": "^7.0.28",
    "apollo-server-koa": "^1.3.2",
    "ava": "^0.25.0",
    "aws-sdk-mock": "^2.0.0",
    "coveralls": "^3.0.1",
    "eslint": "^4.17.0",
    "fs-extra": "^6.0.1",
    "graphql": "^14.0.0-rc.1",
    "graphql-tools": "^3.0.2",
    "koa": "^2.5.0",
    "koa-better-body": "^3.0.4",
    "koa-convert": "^1.2.0",
    "koa-router": "^7.4.0",
    "nyc": "^12.0.1",
    "proxyquire": "^2.0.1",
    "serverless-http": "^1.5.5",
    "sinon": "^5.0.10",
    "uuid": "^3.2.1"
  },
  "dependencies": {
    "@babel/core": "^7.0.0-beta.46",
    "@babel/plugin-transform-async-to-generator": "^7.0.0-beta.49",
    "@babel/polyfill": "^7.0.0-beta.46",
    "@babel/preset-env": "^7.0.0-beta.46",
    "@babel/preset-typescript": "^7.0.0-beta.46",
    "@lifeomic/alpha": "^0.11.0",
    "aws-sdk": "^2.192.0",
    "babel-loader": "^8.0.0-beta.2",
    "dockerode": "^2.5.3",
    "jszip": "^3.1.5",
    "lodash": "^4.17.5",
    "nested-error-stacks": "^2.0.0",
    "promise-retry": "^1.1.1",
    "source-map-support": "^0.5.3",
    "string-replace-loader": "^2.1.1",
    "supertest": "^3.0.0",
    "supports-color": "^5.3.0",
    "uglifyjs-webpack-plugin": "^1.1.8",
    "webpack": "^4.10.0",
    "webpack-babel-env-deps": "^1.4.1",
    "wrapper-webpack-plugin": "^2.0.0",
    "yargs": "^11.0.0",
    "zip-webpack-plugin": "^3.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
