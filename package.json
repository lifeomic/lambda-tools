{
  "name": "@lifeomic/lambda-tools",
  "version": "7.6.2",
  "description": "Common utilities for Lambda testing and development",
  "main": "src/index.js",
  "types": "src/index.d.ts",
  "bin": {
    "lambda-tools-build": "./bin/build.js",
    "lambda-tools-host-addr": "./bin/get-host-addr.js"
  },
  "scripts": {
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "pretest": "eslint .",
    "test": "nyc ava -v"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lifeomic/lambda-tools.git"
  },
  "author": "LifeOmic <development@lifeomic.com>",
  "license": "MIT",
  "ava": {
    "failWithoutAssertions": false,
    "files": [
      "test/**/*.test.js"
    ]
  },
  "eslintConfig": {
    "extends": "plugin:@lifeomic/node/recommended"
  },
  "eslintIgnore": [
    "examples/",
    "test/fixtures/bundled_*",
    "test/fixtures/build"
  ],
  "nyc": {
    "check-coverage": true,
    "lines": 100,
    "statements": 100,
    "functions": 100,
    "branches": 100
  },
  "optionalDependencies": {
    "ts-loader": "*",
    "typescript": "*"
  },
  "devDependencies": {
    "@lifeomic/eslint-plugin-node": "^1.1.1",
    "@types/aws-sdk": "^2.7.0",
    "@types/koa": "^2.0.45",
    "@types/koa-router": "^7.0.28",
    "apollo-server-koa": "^2.0.0",
    "ava": "^1.4.1",
    "aws-sdk-mock": "^4.0.0",
    "coveralls": "^3.0.1",
    "eslint": "^5.1.0",
    "graphql": "^14.0.2",
    "graphql-tools": "^4.0.0",
    "koa": "^2.5.0",
    "koa-router": "^7.4.0",
    "nyc": "^13.1.0",
    "proxyquire": "^2.0.1",
    "serverless-http": "^1.5.5",
    "sinon": "^7.1.0",
    "ts-loader": "^5.3.3",
    "typescript": "^3.3.3333"
  },
  "dependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-transform-async-to-generator": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-typescript": "^7.0.0",
    "@lifeomic/alpha": "^0.14.0",
    "archiver": "^3.0.0",
    "aws-sdk": "^2.313.0",
    "babel-loader": "^8.0.2",
    "chalk": "^2.4.1",
    "dockerode": "^2.5.3",
    "fs-extra": "^7.0.0",
    "glob": "^7.1.2",
    "jszip": "^3.1.5",
    "lodash": "^4.17.5",
    "mockserver-client": "5.5.1",
    "nested-error-stacks": "^2.0.0",
    "promise-retry": "^1.1.1",
    "source-map-support": "^0.5.3",
    "string-replace-loader": "^2.1.1",
    "supertest": "^4.0.1",
    "supports-color": "^6.1.0",
    "terser-webpack-plugin": "^1.1.0",
    "tmp-promise": "^1.0.5",
    "unzipper": "^0.9.11",
    "uuid": "^3.3.2",
    "webpack": "^4.18.0",
    "webpack-babel-env-deps": "^1.4.3",
    "wrapper-webpack-plugin": "^2.0.0",
    "yargs": "^13.2.1",
    "zip-webpack-plugin": "^3.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
