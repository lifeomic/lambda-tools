{
  "name": "@lifeomic/lambda-tools",
  "version": "11.2.1",
  "description": "Common utilities for Lambda testing and development",
  "main": "src/index.js",
  "types": "src/index.d.ts",
  "bin": {
    "lambda-tools-build": "./bin/build.js",
    "lambda-tools-host-addr": "./bin/get-host-addr.js"
  },
  "scripts": {
    "prepublishOnly": "yarn tsc",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "lint": "eslint . --ext .js,.ts -f codeframe",
    "pretest": "yarn lint && tsc --noEmit",
    "test": "nyc ava -v"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lifeomic/lambda-tools.git"
  },
  "author": "LifeOmic <development@lifeomic.com>",
  "license": "MIT",
  "ava": {
    "compileEnhancements": false,
    "failWithoutAssertions": false,
    "timeout": "10m",
    "files": [
      "test/**/*.test.js"
    ],
    "require": [
      "ts-node/register",
      "source-map-support/register"
    ]
  },
  "eslintIgnore": [
    "examples/",
    "test/fixtures/bundled_*",
    "test/fixtures/build"
  ],
  "nyc": {
    "check-coverage": true,
    "all": true,
    "lines": 100,
    "statements": 100,
    "functions": 100,
    "branches": 100,
    "include": [
      "src/**"
    ],
    "exclude": [
      "src/patches/**"
    ]
  },
  "optionalDependencies": {
    "ts-loader": "*",
    "typescript": "*"
  },
  "devDependencies": {
    "@lifeomic/alpha": "^1.2.1",
    "@lifeomic/eslint-plugin-node": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "apollo-server-koa": "^2.0.0",
    "aws-sdk-mock": "^4.0.0",
    "coveralls": "^3.0.1",
    "eslint": "^7.0.0",
    "graphql": "^14.0.2",
    "graphql-tools": "^4.0.0",
    "koa": "^2.5.0",
    "koa-router": "^7.4.0",
    "mockserver-client": "^5.6.1",
    "nyc": "^15.0.1",
    "proxyquire": "^2.0.1",
    "serverless-http": "^2.0.0",
    "sinon": "^7.1.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.10.1",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-async-generator-functions": "^7.7.4",
    "@babel/plugin-transform-async-to-generator": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-typescript": "^7.0.0",
    "@elastic/elasticsearch": "^7.3.0",
    "@types/aws-lambda": "^8.10.51",
    "@types/dockerode": "^2.5.20",
    "@types/fs-extra": "^8.1.0",
    "@types/koa": "^2.0.45",
    "@types/koa-router": "^7.0.28",
    "@types/lodash": "^4.14.150",
    "@types/tmp": "^0.2.0",
    "@types/unzipper": "^0.10.3",
    "@types/uuid": "^7.0.3",
    "archiver": "^3.0.0",
    "ava": "^2.4.0",
    "aws-sdk": "^2.290.0",
    "babel-loader": "^8.0.2",
    "chalk": "^3.0.0",
    "debug": "^4.1.1",
    "dockerode": "^2.5.3",
    "fs-extra": "^8.1.0",
    "glob": "^7.1.2",
    "jszip": "^3.1.5",
    "lodash": "^4.17.5",
    "nested-error-stacks": "^2.0.0",
    "p-queue": "^6.4.0",
    "promise-retry": "^1.1.1",
    "source-map-support": "^0.5.19",
    "string-replace-loader": "^2.1.1",
    "supertest": "^4.0.1",
    "supports-color": "^7.0.0",
    "terser-webpack-plugin": "^2.0.0",
    "tmp-promise": "^2.0.1",
    "unzipper": "^0.10.0",
    "uuid": "^3.3.2",
    "webpack": "^4.41.2",
    "webpack-babel-env-deps": "^1.5.0",
    "wrapper-webpack-plugin": "^2.1.0",
    "yargs": "^15.0.1",
    "zip-webpack-plugin": "^3.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@lifeomic/alpha": "^0.14.0 || ^1.0.0"
  }
}
